[push]
  default = simple
[alias]
s = status
l = log
a = add
aa = add -A
c = commit
cm = commit -m
acm = !git add -A && git commit -m
ca = commit --amend
cam = commit --amend -m
acam = !git add -A && git commit --amend -m
d = diff
r = reset HEAD
p = pull
f = fetch
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
